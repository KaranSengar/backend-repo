{
  "name": "backend",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=production nodemon --watch src --ext ts,json --exec tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "format:fix": "prettier . --write",
    "format:check": "prettier . --check",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:ci": "jest --runInBand --coverage",
    "test2": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watchAll --verbose",
    "test1": "jest --no-cache --coverage --all --runInBand",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d ./src/data-source.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d ./src/data-source.ts",
    "migration-create": "typeorm-ts-node-commonjs migration:create"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@eslint/js": "^9.39.2",
    "@types/axios": "^0.9.36",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/http-errors": "^2.0.5",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.3",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "babel-jest": "^30.2.0",
    "baseline-browser-mapping": "^2.9.14",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "mock-jwks": "^1.0.10",
    "nodemon": "^3.1.11",
    "prettier": "3.7.4",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typeorm-extension": "^3.7.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-jwt": "^8.5.1",
    "express-validator": "^6.15.0",
    "http-errors": "^2.0.1",
    "jsonwebtoken": "^9.0.3",
    "jwks-rsa": "^3.2.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rsa-pem-to-jwk": "^1.1.3",
    "typeorm": "^0.3.28",
    "winston": "^3.19.0"
  }
}
